%% conventions.
%% There are actual occasions and eternal_objects. 
%% nexuus (I use for plural of nexus) are groups of actual
%% occasions indicated by a defining characteristic.
%%
%% people, words, ideas, and other sorts of things are
%% nexuus.
%%
%% Tom would be indicated by his defining characteristic.
%% tom_dc
%% A public word for Tom would be indicated by
%% tom_word_dc
%% Quine's idea of Tom would be indicated by
%% quine_tom_idea_dc.

%% qqqq is theoretically the only precicate needed.
%% I also (for speed anc convenience use qqqq4 and qqqq5).
%% Is used mostly in rules. qqqq4 states positive facts,
%% and qqqq5 states negative facts.

%% rrrr, ssss, and tttt represent prehensions.


%% rrrr( Type, Subject_dc, Time,
%% A_word_dc, A_idea_dc).

%% indicates that the Subject kas a prehrension of
%% A_word_dc in prehension of A_idea_dc

%% ssss( Type, Subject_dc, Time,
%% A_idea_dc, A_object_dc).

%% indicates that the Subject_dc has a prehrension of
%% A_object_dc in prehension of A_idea_dc


%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
dynamic(actual_occasion)?      % sort of like "events"
dynamic(eternal_object)?       % sort of like "universals"
dynamic(defining_characteristic)? % a kind of universal
dynamic(subjective_aim)?       % another kind of universal
dynamic(relation)?             % another kind of universal
dynamic(conceptual_feeling_words)?
dynamic(conceptual_feeling_in_ideas)?
dynamic(conceptual_feeling_objects)?
dynamic(conceptual_feeling_words_ideas)?
dynamic(conceptual_feeling_words_objects)?
dynamic(conceptual_feeling_ideas_objects)?
dynamic(conceptual_feeling_words_ideas_objects)?

eternal_object(X) :- defining_characteristic(X).
eternal_object(X) :- subjective_aim(X).
eternal_object(X) :- relation(X).
nexus(Dc, N) :-
	find_all(X, refer(Dc, X), N).

