rrrr_check1(S, T, W, I) :-
	rrrr(S,T,W,I),
	\+ person(S, N),
	write("Error: have rrrr(", S,
	      ", ", T,
	      ", ", W,
	      ", ", I, " but not person(", S,
	      ",", N, ")").

rrrr_check2(S, T, W, I) :-
	rrrr(S,T,W,I),
	\+ idea_of(S, I),
	write("Error: have rrrr(", S,
	      ", ", T,
	      ", ", W,
	      ", ", I,
	      " but not qqqq(idea_of, ",
	      S,
	      ", ", I, ").").

