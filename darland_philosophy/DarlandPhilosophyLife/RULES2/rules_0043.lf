%% 1

%% top

disbelieves_true_words( Subject, Time, [top, [[Relation_Word] | List]]) :-
	%% list of words
	rrrr( Subject, Time, Relation_Word, Relation_Idea),
	arity_idea( Relation_Idea, Arity),
	type_idea( Relation_Idea, Type),
	match_true_word_disbelief(Subject, Time, List,
			  Arity, Type - 1, [Relation_Idea]).

%% A

disbelieves_true_words( Subject, Time, [nand, List_1, List_2]) :-
	
	write(disbelieves_true_wordsA),
	\+ disbelieves_true_words( Subject, Time, [top, List_1]).

%% B

disbelieves_true_words( Subject, Time, [nand, List_1, List_2]) :-
	write(disbelieves_true_wordsB),
	\+ disbelieves_true_words( Subject, Time, [top, List_2]).

