%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
match_true_word_disbelief(Subject, Time, [], 0, Type, Idea_List) :-
	disbelieves_true_in_ideas( Subject, Time, [top, Idea_List]).

%% -999 indicates arbitrary arity.
match_true_word_disbelief(Subject, Time, [], -999, Idea_List) :-
	disbelieves_true_in_ideas( Subject, Time, [top, Idea_List]).

match_true_word_disbelief(Subject, Time,
		  [[Word] | List],
		  Arity, Type, Idea_List) :-
	% List of Word Idea Pair.
	rrrr( Subject, Time, Word, Idea),
	type_idea( Idea, Type),
	Idea_List_Out = append(Idea_List, [Idea]),
	match_true_word_disbelief(Subject, Time,
			  List,
			  % if Arity is not Arbitrary sub 1
			  cond(Arity =\= -999, Arity - 1,
			       -999),
			  Type, Idea_List_Out).

