match_true_word_idea_disbelief(Subject, Time,
		       [[Word, Idea] | PairList],
		       Arity, Type, Idea_List) :-
	
	% List of Word Idea Object Triple.
	rrrr( Subject, Time, Word, Idea),
	number_idea(Idea, singular),
