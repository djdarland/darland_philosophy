%% list of idea object Pairs
	
match_true_object_disbelief(Subject, Time, [], 0, Type, Idea_List) :- 
	disbelieves_true_in_ideas( Subject, Time, [top, Idea_List]).

%% -999 indicates arbitrary arity.
match_true_object_disbelief(Subject, Time, [], -999, Idea_List) :-
	disbelieves_true_in_ideas( Subject, Time, [top, Idea_List]).

match_true_object_disbelief(Subject, Time,
		    [[Object] | List],
		    Arity, Type, Idea_List) :-
	% List of Objects.
	number_idea(Idea, singular),
	ssss2( Subject, Time, Idea, Object),
	type_idea( Idea, Type),
	Idea_List_Out = append(Idea_List, [Idea]),
	match_true_object_disbelief(Subject, Time,
			    List,
			    % if Arity is not Arbitrary sub 1
			    cond(Arity =\= -999, Arity - 1,
				 -999),
			    Type, Idea_List_Out).

match_true_object_disbelief(Subject, Time,
		    [[Object] | List],
		    Arity, Type, Idea_List) :-
	% List of Objects.
	number_idea(Idea, plural),
	tttt( Subject, Time, Idea, Object),
	type_idea( Idea, Type),
	Idea_List_Out = append(Idea_List, [Idea]),
	match_true_object_disbelief(Subject, Time,
			    List,
			    % if Arity is not Arbitrary sub 1
			    cond(Arity =\= -999, Arity - 1,
				 -999),
			    Type, Idea_List_Out).

