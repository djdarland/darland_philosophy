%% D

disbelieves_true_words( Subject, Time, [nand, [nand, List_1, List_2], List_3]) :-
	write(disbelieves_true_wordsD),
	\+ disbelieves_true_words( Subject, Time, [top, List_3]).

%% E

disbelieves_true_words( Subject, Time, [nand, List_1, [nand, List_2, List_3]]) :-
