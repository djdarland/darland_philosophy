%% conventions.
%% There are actual occasions and eternal_objects. 
%% nexuus (I use for plural of nexus) are groups of actual
%% occasions indicated by a defining characteristic.
%%
%% people, words, ideas, and other sorts of things are
%% nexuus.
%%
%% Tom would be indicated by his defining characteristic.
%% tom_dc
%% A public word for Tom would be indicated by
%% tom_word_dc
%% Quine's idea of Tom would be indicated by
%% quine_tom_idea_dc.

%% qqqq is theoretically the only precicate needed.
%% I also (for speed anc convenience use pos_qq and neg_qq).
%% Is used mostly in rules. pos_qq states positive facts,
%% and neg_qq states negative facts.

%% rrrr, ssss, and tttt represent prehensions.


%% rrrr( Type, Subject_dc, Time,
%% A_word_dc, A_idea_dc).

%% indicates that the Subject kas a prehrension of
%% A_word_dc in prehension of A_idea_dc

%% ssss( Type, Subject_dc, Time,
%% A_idea_dc, A_object_dc).

%% indicates that the Subject_dc has a prehrension of
%% A_object_dc in prehension of A_idea_dc


%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
qqqq(dynamic, actual_occasion)?      % sort of like "events"
qqqq(dynamic, eternal_object)?       % sort of like "universals"
qqqq(dynamic, defining_characteristic)? % a kind of universal
qqqq(dynamic, subjective_aim)?       % another kind of universal
% qqqq(dynamic, relation)?             % another kind of universal
qqqq(dynamic, conceptual_feeling_words)?
qqqq(dynamic, conceptual_feeling_in_ideas)?
qqqq(dynamic, conceptual_feeling_objects)?
qqqq(dynamic, conceptual_feeling_words_ideas)?
qqqq(dynamic, conceptual_feeling_words_objects)?
qqqq(dynamic, conceptual_feeling_ideas_objects)?
qqqq(dynamic, conceptual_feeling_words_ideas_objects)?

qqqq(eternal_object, X) :- qqqq(defining_characteristic, X).
qqqq(eternal_object, X) :- qqqq(subjective_aim, X).
qqqq(eternal_object, X) :- qqqq(relation, X).
qqqq(nexus, Dc, N) :- qqqq(find_all, X, qqqq(refer, Dc, X), N).

