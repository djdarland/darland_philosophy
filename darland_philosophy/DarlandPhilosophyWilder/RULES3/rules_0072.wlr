qqqq(match_sa_object, Subject, Subjective_aim, Time,
		[[Object] | List],
		Arity, Type, Idea_List) :-
	% List of Objects.
	qqqq(number_idea, Idea, singular),
	qqqq(ssss2, Subject, Time, Idea, Object),
	qqqq(type_idea, Idea, Type),
	Idea_List_Out = append(Idea_List, [Idea]),
	qqqq(match_sa_object, Subject,
			Subjective_aim, Time,
			List,
			% if Arity is not Arbitrary sub 1
			qqqq(cond, Arity =\= -999, Arity - 1,
			     -999),
			Type, Idea_List_Out).

