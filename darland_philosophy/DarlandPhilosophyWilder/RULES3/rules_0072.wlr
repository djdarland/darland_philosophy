	qqqq(arity_idea, Relation_Idea, Arity),
	qqqq(type_idea, Relation_Idea, Type),
	qqqq(match_sa_object, Subject, Subjective_aim,
			Time, List,
			Arity, Type - 1, [Relation_Idea]).

